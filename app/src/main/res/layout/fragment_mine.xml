    <RelativeLayout
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_title"
        android:background="#ffffff"
        android:layout_marginTop="@dimen/x6"
        >
     <RelativeLayout
         android:id="@+id/member"
         android:layout_width="match_parent"
         android:layout_height="@dimen/x40"
         android:background="#ffffff"
         >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="设计师入驻"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
         <ImageView
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:background="@drawable/more"
             android:layout_centerVertical="true"
             android:layout_alignParentRight="true"
             android:layout_marginRight="@dimen/x18"
             />
     </RelativeLayout>
       <View
           android:id="@+id/member_line"
           android:layout_width="match_parent"
           android:layout_height="1dp"
           android:layout_below="@+id/member"
           android:background="#f4f4f4"
           android:layout_marginRight="@dimen/x18"
           android:layout_marginLeft="@dimen/x18"
           />


    <RelativeLayout
        android:id="@+id/friend"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:layout_below="@+id/member_line"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="会员中心"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    <View
        android:id="@+id/friend_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/friend"
        android:background="#f4f4f4"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        />

    <RelativeLayout
        android:id="@+id/dynamic"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:layout_below="@+id/friend_line"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="我的收藏"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />

    </RelativeLayout>
        <View
            android:id="@+id/dynamic_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/dynamic"
            android:background="#f4f4f4"
            android:layout_marginRight="@dimen/x18"
            android:layout_marginLeft="@dimen/x18"
            />
        <RelativeLayout
            android:id="@+id/video"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x40"
            android:layout_below="@+id/dynamic_line"
            android:background="#ffffff"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="视频缓存"
                android:layout_marginLeft="@dimen/x18"
                android:textSize="18sp"
                android:textColor="#303030"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/more"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x18"
                />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/one"
        android:background="#ffffff"
        android:layout_marginTop="@dimen/x6"
        >
    <RelativeLayout
        android:id="@+id/send_orders"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="我的订单"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    <View
        android:id="@+id/send_orders_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/send_orders"
        android:background="#f4f4f4"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        />


    <RelativeLayout
        android:id="@+id/make"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:layout_below="@+id/send_orders_line"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="我收到的派单"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    <View
        android:id="@+id/make_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/make"
        android:background="#f4f4f4"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        />


    <RelativeLayout
        android:id="@+id/projeck"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:layout_below="@+id/make_line"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="我收到的预约"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/two"
        android:background="#ffffff"
        android:layout_marginTop="@dimen/x6"
        >
    <RelativeLayout
        android:id="@+id/currency"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:background="#ffffff"

        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/currency"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    <View
        android:id="@+id/currency_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/currency"
        android:background="#f4f4f4"
        android:layout_marginRight="@dimen/x18"
        android:layout_marginLeft="@dimen/x18"
        />

    <RelativeLayout
        android:id="@+id/set_"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x40"
        android:layout_below="@+id/currency_line"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/set_"
            android:layout_marginLeft="@dimen/x18"
            android:textSize="18sp"
            android:textColor="#303030"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x18"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/x60"
        android:layout_below="@+id/set_"
        android:background="#f5f5f5"
        />
    </RelativeLayout>
</RelativeLayout>
